.custom-label {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(68, 138, 255);
}

.message-label:hover {
    scale: 1.02 1.02;
    transition-duration: 0.05s;
    transition-timing-function: linear;
}

.message-label {
    scale: 1 1;
    transition-duration: 0.05s;
    transition-timing-function: linear;
}
